{
  "name": "froutes",
  "description": "file tree based rest api routes for node",
  "version": "0.0.0",
  "repository": "git@github.com:barelyhuman/froutes.git",
  "author": "Reaper <ahoy@barelyhuman.dev>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "main.js",
  "files": [
    "main.js",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./main.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "esbuild main.js --minify --bundle --outfile=dist/index.js --platform=node --target=node12"
  },
  "devDependencies": {
    "esbuild": "^0.14.14"
  }
}
